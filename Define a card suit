function defineSuit(card) {
  // good luck
  let suit=card.charAt(card.length-1);
  switch (suit) {
      case "♣":
      return "clubs";
      break;
      case "♦":
      return "diamonds";
      break;
      case "♥":
      return "hearts";
      break;
      case "♠":
      return "spades";
      break;
  }
}
