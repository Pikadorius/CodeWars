function moveZeros(arr) {
  let newArr=arr.filter(item=>item!==0);
  let shift=arr.length-newArr.length;
  for (let i=0; i<shift; i++) {
    newArr.push(0);
  }
  return newArr;
}


____________________________________

function moveZeros(arr) {
  return arr.filter(x=>x!==0).concat(arr.filter(x=>x===0));
}
